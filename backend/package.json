{
  "name": "music-radio",
  "version": "0.0.1",
  "description": "Inspired by [SyncMusic](https://github.com/kasuganosoras/SyncMusic)",
  "scripts": {
    "dev": "cross-env NODE_ENV=development SESSION_TYPE=token nodemon -e js,mjs,json,ts",
    "debug": "cross-env NODE_ENV=development SESSION_TYPE=token node --inspect -r tsconfig-paths/register -r ts-node/register app.ts",
    "start": "ts-node -r tsconfig-paths/register app.ts",
    "build": "tsc --build tsconfig.json",
    "test": "jest -i --coverage --forceExit",
    "server": "cross-env STATIC_PATH=./static NODE_ENV=production node ./build/backend/app.js",
    "runts": "ts-node -r tsconfig-paths/register"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanmmm/MusicRadio.git"
  },
  "keywords": [
    "music",
    "radio",
    "nodejs",
    "typescript"
  ],
  "author": "sanmmm",
  "license": "MIT",
  "dependencies": {
    "@jest/types": "^25.5.0",
    "classnames": "^2.2.6",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cron": "^1.7.2",
    "cross-env": "^7.0.2",
    "deep-equal": "^2.0.3",
    "express": "^4.17.1",
    "express-react-views": "^0.11.0",
    "got": "^9.6.0",
    "ioredis": "^4.14.1",
    "jss": "^10.1.1",
    "jss-preset-default": "^10.1.1",
    "mint-filter": "^3.0.0",
    "polished": "^3.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "socket.io": "^2.5.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/got": "^9.6.9",
    "@types/ioredis": "^4.16.0",
    "@types/jest": "^25.2.1",
    "@types/jquery": "^3.3.35",
    "@types/react": "^16.9.34",
    "@types/socket.io": "^2.1.4",
    "@types/typescript": "^2.0.0",
    "cheerio": "^1.0.0-rc.3",
    "form-data": "^3.0.0",
    "jest": "^25.5.4",
    "nodemon": "^1.19.4",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.5.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/test/*.ts",
      "!**/_*.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/static/",
      "/views/"
    ],
    "collectCoverageFrom": [
      "lib/**/*.ts"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "moduleNameMapper": {
      "^root/(.*)$": "<rootDir>/$1",
      "^global/(.*)$": "<rootDir>/../$1"
    }
  }
}
